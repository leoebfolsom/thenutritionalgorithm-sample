<% if @eligible_foods.length != 0 %>
  $("#browse-results-container").html("");
  <% @eligible_foods.each do |f| %>
    $("#browse-results-container").append("<%= j render 'shared/search_result', :f => Food.find(f), l: @list_id, from: 'browse' %>");
  <% end %>

<% else %>
  $("#browse-results-container").html("There are no remaining foods in this category.");
<% end %>
